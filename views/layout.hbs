        {{#if nav}}
            <div class="header nav" id="myHeader">
                <a title="Home Button" href="/"><div class="fasLogo"></div></a>
                <ul>
                  <li><a href="javascript:void(0);" title="Open Nav" onclick="openNav()" class="burger">☰</a></li>
                </ul>
            </div>
            <!-- open/close -->
            <!-- The overlay -->
            <div id="myNav" class="overlay">

              <!-- Button to close the overlay navigation -->
              

              <!-- Overlay content -->
              <div class="overlay-content">

                <a title="Nav Inhabitants" href="/Inhabitants"><h3>INHABITANTS</h3></a>
                <a title="Nav Formation" href="/formation"><h3>FORMATION</h3></a>
                <a title="Nav Interactive" href="/interactive"><h3>INTERACTIVE</h3></a>
                <a title="Nav About" href="/about"><h3>ABOUT</h3></a>
                <a title="Nav Subscribe" href="/emails/join"><h3>SUBSCRIBE</h3></a>
                
              </div>
              <a href="javascript:void(0);" class="closebtn" title="Close Nav" onclick="closeNav()">&times;</a>
            </div>
            <script type="text/javascript">

                function openNav() {
                  document.getElementById("myNav").style.display = "block";
                  console.log("open");
                }

                /* Close */
                function closeNav() {
                  document.getElementById("myNav").style.display = "none";
                  console.log("close");
                }

            </script>

        {{/if}}

        {{#if intro}}
            <div class="introCopy">
                <div class="introParent">
                    <div>
                        <h1>ECO-</h1>
                    </div>
                    <div id="wordRotate">
                        <h1>INFORM</h1>
                    </div>
                </div>
            </div>
            <script type="text/javascript">
                var textSwap = document.getElementById("wordRotate");
                var introCont = document.querySelector(".introCopy");
                //var cookieVar = document.cookie;
                //console.log(document.cookie);

                function initAos() {
                    AOS.init({
                            duration: 1000,
                            easing: 'ease-in-out-quad', 
                            once: false,
                            mirror: false
                    });
                };
                // intro copy change
                if ( document.cookie ){
                    introCont.style.display = "none";
                    initAos();
                } else {
                    setTimeout(function(){
                        textSwap.querySelector("h1").innerHTML = "PROTECT";
                    },1000);
                    setTimeout(function(){
                        textSwap.querySelector("h1").innerHTML = "FIRST";
                    },2000);
                    setTimeout(function(){
                        introCont.classList.add("intro-scale");
                        document.cookie = "visited=true";
                    },4000);
                    initAos();
                }
            </script>

        {{/if}}

        {{#if footer}}
        <footer class="row">
