        {{#if intro}}
            <div class="introCopy">
                <div class="introParent">
                    <div>
                        <h1>ECO-</h1>
                    </div>
                    <div id="wordRotate">
                        <h1>INFORM</h1>
                    </div>
                </div>
            </div>
            <script type="text/javascript">
                var textSwap = document.getElementById("wordRotate");
                var introCont = document.querySelector(".introCopy");
                //var cookieVar = document.cookie;
                //console.log(document.cookie);

                function initAos() {
                    AOS.init({
                            duration: 1000,
                            easing: 'ease-in-out-quad', 
                            once: false,
                            mirror: false
                    });
                };
                // intro copy change
                if ( document.cookie ){
                    introCont.style.display = "none";
                    initAos();
                } else {
                    setTimeout(function(){
                        textSwap.querySelector("h1").innerHTML = "PROTECT";
                    },1000);
                    setTimeout(function(){
                        textSwap.querySelector("h1").innerHTML = "FIRST";
                    },2000);
                    setTimeout(function(){
                        introCont.classList.add("intro-scale");
                        document.cookie = "visited=true";
                    },4000);
                    initAos();
                }
            </script>

        {{/if}}

